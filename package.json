{
  "name": "auth-service",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -- turbo dev --parallel --continue --no-cache",
    "start": "dotenv -- turbo start --parallel --continue --no-cache",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean",
    "prepare": "husky",
    "type-check": "dotenv -- turbo type-check --parallel --continue --no-cache"
  },
  "devDependencies": {
    "dotenv-cli": "^7.4.2",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "turbo": "latest"
  },
  "packageManager": "pnpm@9.1.2",
  "engines": {
    "node": ">=18"
  }
}
